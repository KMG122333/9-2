#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <string.h>
#include <stdlib.h>

int main(){
    char s1[20] = "world";
    char *s2 = (char *)malloc(sizeof(char) * 20);//c++컴파일러에서는 (char *) 없이는 오류가 발생

    strcat(s2, "Hello");

    strcat(s2, s1);
    
    printf("%s\n", s2);

    free(s2);
    
    return 0;
}
